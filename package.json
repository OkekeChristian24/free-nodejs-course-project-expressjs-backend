{
	"name": "blog-expressjs",
	"version": "1.0.0",
	"main": "dist/server.js",
	"scripts": {
		"dev": "npx ts-node-dev --files src/server.ts",
		"dev:all": "docker compose -f docker-compose.dev.yml up -d && npx ts-node-dev --files src/server.ts",
		"start": "rm -rf dist && tsc && node dist/server.js",
		"start:all": "rm -rf dist && tsc && docker compose up -f docker-compose.prod.yml -d && node dist/server.js",
		"build": "rm -rf dist && tsc",
		"type-check": "npx tsc --noEmit",
		"docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
		"docker:prod:up": "docker compose -f docker-compose.prod.yml up -d",
		"docker:dev:down": "docker compose -f docker-compose.dev.yml down",
		"docker:prod:down": "docker compose -f docker-compose.prod.yml down",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"express-validator": "^7.2.1",
		"ioredis": "^5.8.2",
		"jsonwebtoken": "^9.0.2",
		"kafkajs": "^2.2.4",
		"mysql2": "^3.14.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.9.2"
	}
}
